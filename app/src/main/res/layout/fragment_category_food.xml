<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#8cc54a ">

    <ScrollView
        android:id="@+id/Scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/bt_back"
                android:layout_width="72dp"
                android:layout_height="82px"
                android:layout_marginTop="17dp"
                android:layout_marginRight="155dp"
                android:background="@drawable/back_bt_background"
                android:fontFamily="@font/wemakeprice_regular"
                android:gravity="center"
                android:text="&lt; BACK"
                android:textSize="40px" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/imcresoojin"
                android:gravity="center"
                android:text="음식물 쓰레기"
                android:textColor="#000000"
                android:textSize="65px" />
            
            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#000000"
                android:layout_marginTop="35dp"
                />

            <TextView
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:text="- ㉠~㉡"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/imcresoojin"
                android:textColor="@color/black"
                android:textAlignment="textStart"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="15dp">
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="갑각류 껍데기"
                    android:id="@+id/crab_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    android:layout_marginRight="20dp"
                    />
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="견과류 껍데기"
                    android:id="@+id/nuts_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="15dp">
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="귤 껍질"
                    android:id="@+id/tangerine_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    android:layout_marginRight="20dp"
                    />
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="과일 씨앗"
                    android:id="@+id/fruit_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <TextView
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:text="- ㉢~㉣ "
                android:layout_marginTop="10dp"
                android:fontFamily="@font/imcresoojin"
                android:textColor="@color/black"
                android:textAlignment="textStart"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="15dp">
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="동물 털 및 뼈다귀"
                    android:id="@+id/bone_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    android:layout_marginRight="20dp"
                    />
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="달걀"
                    android:id="@+id/egg_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="15dp"/>
            <TextView
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:text="- ㉦~㉧ "
                android:layout_marginTop="10dp"
                android:fontFamily="@font/imcresoojin"
                android:textColor="@color/black"
                android:textAlignment="textStart"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="15dp">
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="생선뼈"
                    android:id="@+id/fish_bone_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    android:layout_marginRight="20dp"
                    />
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="식물 뿌리"
                    android:id="@+id/root_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="15dp">
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="양파껍질"
                    android:id="@+id/onion_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    android:layout_marginRight="20dp"
                    />
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="옥수수껍질"
                    android:id="@+id/corn_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <TextView
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:text="- ㉨~㉩ "
                android:layout_marginTop="10dp"
                android:fontFamily="@font/imcresoojin"
                android:textColor="@color/black"
                android:textAlignment="textStart"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="15dp">
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="조개,전복 등 패류 껍데기"
                    android:id="@+id/shellfish_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    android:layout_marginRight="20dp"
                    />
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="젓갈류"
                    android:id="@+id/salted_fish_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="15dp"/>
            <TextView
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:text="- ㉬~㉭ "
                android:layout_marginTop="10dp"
                android:fontFamily="@font/imcresoojin"
                android:textColor="@color/black"
                android:textAlignment="textStart"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_marginTop="15dp">
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="핵과류의 씨"
                    android:id="@+id/seed_of_nuclear_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    android:layout_marginRight="20dp"
                    />
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/back_bt_background"
                    android:text="풍선껌(껌)"
                    android:id="@+id/gum_bt"
                    android:fontFamily="@font/imcresoojin"
                    android:textColor="@color/white"
                    />
            </LinearLayout>

        </LinearLayout>

    </ScrollView>


</LinearLayout>